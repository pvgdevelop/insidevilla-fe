  %body
    %header
      .row
        .columns.large-3.small-6
          %a.agency_logo{:href => "index.html"}
            %img{:src => "images/logo.png"}
        .columns.large-3.small-6
          %span.agency_description Агентство недвижимости <br> о. Пхукет
        .columns.large-6.text-right.small-12
          .agency_contacts
            %span.agency_mail
              %i.fa.fa-envelope
              %a{:href => "mailto:info@insidevilla.com "} info@insidevilla.com
            %span.agency_phones
              Viber
              %a{:href => "callto:+66947769961"} +66947769961
              , WhatsApp
              %a{:href => "callto:+66943511068"} +66943511068
      %section.main_menu#main_menu
        .row
          .columns.large-12.small-12
            %nav
              %ul.menu
                %li.active
                  %a{:href => "catalogue.html"} Аренда
                %li
                  %a{:href => "catalogue.html"} Продажа
                %li
                  %a{:href => "#"} Добавить
                %li
                  %a{:href => "services.html"} Услуги
                %li
                  %a{:href => "#"} Инфо
                %li
                  %a{:href => "#"} О&nbsp;нас

    %section.catalogue_filter.clearfix#catalogue_filter
      .row
        .columns.large-12
          .filter_form
            %h3 Поиск
            %form
              .columns.large-10.small-12.filter
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") Статус:
                  .columns.large-10.small-12
                    %select
                      %option Аренда
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") Район:
                  .columns.large-10.small-12
                    %select
                      %option Любой
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") Тип:
                  .columns.large-10.small-12
                    %select
                      %option Все типы
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") Спален:
                  .columns.large-10.small-12
                    %select
                      %option Любое количество
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") Ванных:
                  .columns.large-10.small-12
                    %select
                      %option Любое количество
                .columns.large-4.small-12
                  .columns.large-2.small-12
                    %label(for="right-label") До&nbsp;моря:
                  .columns.large-10.small-12
                    %select
                      %option Любое расстояние
                .row
                  .columns.large-1.small-12
                    %label(for="left-label") Цена:
                  .columns.large-11.small-12.price_catalogue_filter
                    .row.collapse
                      .small-6.large-3.columns
                        %input{:placeholder => "Цена от", :type => "text"}
                      .small-6.large-3.columns
                        %input{:placeholder => "Цена до", :type => "text"}
                      .small-12.large-6.columns
                        %ul.button-group.round.toggle.price_radio_group{"data-toggle" => "buttons-radio"}
                          %li
                            %input#r1{"data-toggle" => "button", :name => "r-group", :type => "radio"}
                              %label.button{:for => "r1"} бат/месяц
                          %li
                            %input#r2{"data-toggle" => "button", :name => "r-group", :type => "radio"}
                              %label.button{:for => "r2"} бат/сутки
                    .layout-slider
                      %input#Slider3{:name => "area", :type => "slider", :value => "25;75"}/
                    %script{:charset => "utf-8", :type => "text/javascript"}
                      jQuery("#Slider3").slider({
                      from: 0,
                      to:300,
                      heterogeneity: ['50/100', '75/250'],
                      limits: false,
                      step: 1,
                      skin: "round_plastic" });
                .columns.large-12.small-12
                  %h5.advanced_search_open Расширенный поиск
                  %hr
                  .advanced_search{:style=>"display:none;"}
                    .columns.large-3.small-12
                      %div.checkbox
                        %input#checkbox1(type="checkbox")
                          %label(for="checkbox1") Бассейн
                      %div.checkbox
                        %input#checkbox2(type="checkbox")
                          %label(for="checkbox2") Вид на море
                      %div.checkbox
                        %input#checkbox3(type="checkbox")
                          %label(for="checkbox3") Вид на горы
                    .columns.large-3.small-12
                      %div.checkbox
                        %input#checkbox4(type="checkbox")
                          %label(for="checkbox4") Спортзал
                      %div.checkbox
                        %input#checkbox5(type="checkbox")
                          %label(for="checkbox5") Паркинг
                      %div.checkbox
                        %input#checkbox6(type="checkbox")
                          %label(for="checkbox6") Охраняемый комплекс
                    .columns.large-3.small-12.end
                      %div.checkbox
                        %input#checkbox7(type="checkbox")
                          %label(for="checkbox7") Тропический сад
                      %div.checkbox
                        %input#checkbox8(type="checkbox")
                          %label(for="checkbox8") Барбекю
                      %div.checkbox
                        %input#checkbox9(type="checkbox")
                          %label(for="checkbox9") Повар
              .columns.large-2.small-12
                %button.button.button_search.button-crimson
                  %i.fa.fa-search
                  Поиск
                .button.button_search.id_search Поиск по ID
                .row.collapse.postfix-round.id_search_form{:style=>"display:none;"}
                  .small-9.columns
                    %input{:placeholder => "ID", :type => "text"}
                  .small-3.columns
                    %a.button.id_search_button.postfix{:href => "#"}
                      .fa.fa-search
